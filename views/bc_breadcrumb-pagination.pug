include includes/header.pug
block content
  body
    // [ Pre-loader ] start
    .loader-bg
      .loader-track
        .loader-fill
    // [ Pre-loader ] End
    include includes/sidenav.pug
    include includes/navbar-nav.pug
    // [ Main Content ] start
    .pcoded-main-container
      .pcoded-wrapper
        .pcoded-content
          .pcoded-inner-content
            // [ breadcrumb ] start
            .page-header
              .page-block
                .row.align-items-center
                  .col-md-12
                    .page-header-title
                      h5.m-b-10 Breadcrumbs & Pagination
                    ul.breadcrumb
                      li.breadcrumb-item
                        a(href='index.html')
                          i.feather.icon-home
                      li.breadcrumb-item
                        a(href='javascript:') Basic Componants
                      li.breadcrumb-item
                        a(href='javascript:') Breadcrumbs & Pagination
            // [ breadcrumb ] end
            .main-body
              .page-wrapper
                // [ Main Content ] start
                .row
                  // [ Breadcrumbs & Pagination ] start
                  .col-sm-6
                    .card
                      .card-header
                        h5 Breadcrumb
                      .card-body
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item.active(aria-current='page') Home
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item
                              a(href='#!') Home
                            li.breadcrumb-item.active(aria-current='page') Library
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item
                              a(href='#!') Home
                            li.breadcrumb-item
                              a(href='#!') Library
                            li.breadcrumb-item.active(aria-current='page') Data
                  .col-sm-6
                    .card
                      .card-header
                        h5 Breadcrumb Icon
                      .card-body
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item.active(aria-current='page')
                              i.feather.icon-home
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item
                              a(href='#!')
                                i.feather.icon-home
                            li.breadcrumb-item.active(aria-current='page') Library
                        nav(aria-label='breadcrumb')
                          ol.breadcrumb
                            li.breadcrumb-item
                              a(href='#!')
                                i.feather.icon-home
                            li.breadcrumb-item
                              a(href='#!') Library
                            li.breadcrumb-item.active(aria-current='page') Data
                  .col-sm-12
                    .card
                      .card-header
                        h5 Pagination
                      .card-body
                        nav(aria-label='Page navigation example')
                          ul.pagination
                            li.page-item
                              a.page-link(href='#!') Previous
                            li.page-item
                              a.page-link(href='#!') 1
                            li.page-item
                              a.page-link(href='#!') 2
                            li.page-item
                              a.page-link(href='#!') 3
                            li.page-item
                              a.page-link(href='#!') Next
                        h5.mt-5 Working With Icons
                        hr
                        nav(aria-label='Page navigation example')
                          ul.pagination
                            li.page-item
                              a.page-link(href='#!' aria-label='Previous')
                                span(aria-hidden='true') «
                                span.sr-only Previous
                            li.page-item
                              a.page-link(href='#!') 1
                            li.page-item
                              a.page-link(href='#!') 2
                            li.page-item
                              a.page-link(href='#!') 3
                            li.page-item
                              a.page-link(href='#!' aria-label='Next')
                                span(aria-hidden='true') »
                                span.sr-only Next
                        h5.mt-5 Disabled and Active States
                        hr
                        nav(aria-label='...')
                          ul.pagination
                            li.page-item.disabled
                              span.page-link Previous
                            li.page-item
                              a.page-link(href='#!') 1
                            li.page-item.active
                              span.page-link
                                | 2
                                span.sr-only (current)
                            li.page-item
                              a.page-link(href='#!') 3
                            li.page-item
                              a.page-link(href='#!') Next
                  // [ Breadcrumbs & Pagination ] end
                  // [ Main Content ] end
                  // [ Main Content ] end
                  // Warning Section Starts
                  // Older IE warning message
                  //if lt IE 11
                    .ie-warning
                      h1 Warning!!
                      p
                        | You are using an outdated version of Internet Explorer, please upgrade
                        br
                        | to any of the following web browsers to access this website.
                      .iew-container
                        ul.iew-download
                          li
                            a(href='http://www.google.com/chrome/')
                              img(src='assets/images/browser/chrome.png' alt='Chrome')
                              div Chrome
                          li
                            a(href='https://www.mozilla.org/en-US/firefox/new/')
                              img(src='assets/images/browser/firefox.png' alt='Firefox')
                              div Firefox
                          li
                            a(href='http://www.opera.com')
                              img(src='assets/images/browser/opera.png' alt='Opera')
                              div Opera
                          li
                            a(href='https://www.apple.com/safari/')
                              img(src='assets/images/browser/safari.png' alt='Safari')
                              div Safari
                          li
                            a(href='http://windows.microsoft.com/en-us/internet-explorer/download-ie')
                              img(src='assets/images/browser/ie.png' alt='')
                              div IE (11 & above)
                      p Sorry for the inconvenience!
                  // Warning Section Ends
                  // Required Js
                  include includes/footer.pug
