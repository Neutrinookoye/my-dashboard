include includes/header.pug
block content
  body
    // [ Pre-loader ] start
    .loader-bg
      .loader-track
        .loader-fill
    // [ Pre-loader ] End
    include includes/sidenav.pug
    include includes/navbar-nav.pug
    // [ Main Content ] start
    .pcoded-main-container
      .pcoded-wrapper
        .pcoded-content
          .pcoded-inner-content
            // [ breadcrumb ] start
            .page-header
              .page-block
                .row.align-items-center
                  .col-md-12
                    .page-header-title
                      h5.m-b-10 feather
                    ul.breadcrumb
                      li.breadcrumb-item
                        a(href='index.html')
                          i.feather.icon-home
                      li.breadcrumb-item
                        a(href='javascript:') Icons
                      li.breadcrumb-item
                        a(href='javascript:') Feather icon
            // [ breadcrumb ] end
            .main-body
              .page-wrapper
                // [ Main Content ] start
                .row
                  .col-sm-12
                    .card
                      .card-header
                        h5 Feather Icon
                      .card-block
                        .row.justify-content-center
                          .col-sm-6
                            input#icon-search.form-control.mb-4(type='text' placeholder='search . . ')
                        #icon-wrapper.i-main
                // [ Main Content ] end
                // [ Main Content ] end
                // Warning Section Starts
                // Older IE warning message
                //if lt IE 11
                  .ie-warning
                    h1 Warning!!
                    p
                      | You are using an outdated version of Internet Explorer, please upgrade
                      br
                      | to any of the following web browsers to access this website.
                    .iew-container
                      ul.iew-download
                        li
                          a(href='http://www.google.com/chrome/')
                            img(src='assets/images/browser/chrome.png' alt='Chrome')
                            div Chrome
                        li
                          a(href='https://www.mozilla.org/en-US/firefox/new/')
                            img(src='assets/images/browser/firefox.png' alt='Firefox')
                            div Firefox
                        li
                          a(href='http://www.opera.com')
                            img(src='assets/images/browser/opera.png' alt='Opera')
                            div Opera
                        li
                          a(href='https://www.apple.com/safari/')
                            img(src='assets/images/browser/safari.png' alt='Safari')
                            div Safari
                        li
                          a(href='http://windows.microsoft.com/en-us/internet-explorer/download-ie')
                            img(src='assets/images/browser/ie.png' alt='')
                            div IE (11 & above)
                    p Sorry for the inconvenience!
